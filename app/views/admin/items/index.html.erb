<table border="1">
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item|%>
    <tr>
      <td><%= item.id %></td>
      <td>
        <%= link_to item.name , admin_end_user_path(item.id) %>
      </td>
      <td><%= item.price %></td>
      <td><%= item.genre.title %></td>
      <td><%= item.sales_status %></td>
    </tr>
    <% end %>
  </tbody>
</table>
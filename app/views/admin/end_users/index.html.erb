<h4>会員一覧</h4>

<table border="1">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user|%>
    <tr>
      <td><%= user.id %></td>
      <td>
        <%= link_to admin_end_user_path(user.id) do %>
          <%= user.last_name %> <%= user.first_name %>
        <% end %>
      </td>
      <td><%= user.email %></td>
      <td><%= user.is_deleted %></td>
    </tr>
    <% end %>
  </tbody>
</table>

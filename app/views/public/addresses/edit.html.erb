<h1>配送先編集</h1>
<%= form_for(@address, url: address_path, method: :patch) do |f| %>
    <div class="field">
        <%= f.label :郵便番号 %>
        <%= f.text_field :post_code %>
    </div>
    <div class="field">
        <%= f.label :住所 %>
        <%= f.text_area :address %>
    </div>
    <div class="field">
        <%= f.label :宛名 %>
        <%= f.text_field :name %>
    </div>
    <%= f.submit "編集する" %>
<% end %>